pipeline
{
agent any
{
stages
{
stage('scm checkout')
{
steps
{
git 'https://github.com/devinameena/maven-project.git'
}
}
stage('compile code')
{
steps
{
withMaven(jdk: 'LocalJDK', maven: 'LocalMaven')
{
   sh 'mvn compile'
}
}
}
stage('test')
{
steps
{
withMaven(jdk: 'LocalJDK', maven: 'LocalMaven')
sh 'mvn test'
}
}
}
}
}
